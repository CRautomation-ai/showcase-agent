OPENAI_API_KEY=sk-your-openai-api-key

# Postgres connection string (Neon or any Postgres with pgvector)
# Get it: https://neon.tech (see setup below)
POSTGRES_URL=postgresql://user:password@host/dbname?sslmode=require

# Auth: global password and JWT (required for protected routes)
ACCESS_PASSWORD=your-shared-company-password
JWT_SECRET=your-secret-to-sign-jwts

# OPTIONAL (defaults shown)

OPENAI_MODEL=gpt-3.5-turbo
OPENAI_EMBEDDING_MODEL=text-embedding-3-large
# EMBEDDING_DIM=3072